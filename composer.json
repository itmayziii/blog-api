{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.2.0",
    "ext-mongodb": "^1.5",
    "laravel/lumen-framework": "^5.6",
    "vlucas/phpdotenv": "~2.2",
    "nordsoftware/lumen-cors": "^2.1",
    "neomerx/json-api": "^2.0",
    "illuminate/redis": "^5.6",
    "doctrine/dbal": "^2.7",
    "mongodb/mongodb": "^1.4"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": [
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": "vendor/bin/phpunit --stop-on-failure"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.2.2"
    }
  }
}
